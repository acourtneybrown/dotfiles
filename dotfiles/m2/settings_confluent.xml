<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                       https://maven.apache.org/xsd/settings-1.0.0.xsd">
    <mirrors>
        <mirror>
            <id>confluent-codeartifact-internal</id>
            <mirrorOf>central,java.net,findify,jcenter,confluent,confluent-artifactory-internal,confluent-artifactory-central</mirrorOf>
            <url>https://confluent-519856050701.d.codeartifact.us-west-2.amazonaws.com/maven/maven/</url>
        </mirror>
    </mirrors>
    <servers>
        <server>
            <id>confluent-codeartifact-central</id>
            <username>aws</username>
            <password>${env.CODEARTIFACT_AUTH_TOKEN}</password>
        </server>
        <server>
            <id>confluent-codeartifact-internal</id>
            <username>aws</username>
            <password>${env.CODEARTIFACT_AUTH_TOKEN}</password>
        </server>
        <server>
            <id>confluent-codeartifact-internal-dev</id>
            <username>aws</username>
            <password>${env.CODEARTIFACT_AUTH_TOKEN}</password>
        </server>
        <server>
            <id>confluent-codeartifact-stabilization</id>
            <username>aws</username>
            <password>${env.CODEARTIFACT_AUTH_TOKEN}</password>
        </server>
    </servers>
    <profiles>
        <profile>
            <id>confluent-codeartifact</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <id>confluent-codeartifact-central</id>
                    <url>https://confluent-519856050701.dp.confluent.io/maven/maven-public/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>confluent-codeartifact-internal</id>
                    <url>https://confluent-519856050701.d.codeartifact.us-west-2.amazonaws.com/maven/maven/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>confluent-codeartifact-central</id>
                    <name>Confluent Plugin Repository</name>
                    <url>https://confluent-519856050701.dp.confluent.io/maven/maven-public/</url>
                    <layout>default</layout>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                </pluginRepository>
                <pluginRepository>
                    <id>confluent-codeartifact-internal</id>
                    <name>Confluent Plugin Repository</name>
                    <url>https://confluent-519856050701.d.codeartifact.us-west-2.amazonaws.com/maven/maven/</url>
                    <layout>default</layout>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                </pluginRepository>
            </pluginRepositories>
        </profile>
        <profile>
            <id>confluent-codeartifact-nonprod</id>
            <activation>
                <property>
                    <name>env.CODE_ARTIFACT_USE_NONPROD</name>
                    <value>true</value>
                </property>
            </activation>
            <repositories>
                <repository>
                    <id>confluent-codeartifact-internal-dev</id>
                    <url>https://confluent-755363985185.d.codeartifact.us-west-2.amazonaws.com/maven/maven-dev/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>confluent-codeartifact-internal-dev</id>
                    <name>Confluent Plugin Repository</name>
                    <url>https://confluent-755363985185.d.codeartifact.us-west-2.amazonaws.com/maven/maven-dev/</url>
                    <layout>default</layout>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                </pluginRepository>
            </pluginRepositories>
        </profile>
        <profile>
            <id>confluent-codeartifact-release-stabilization</id>
            <activation>
                <property>
                    <name>env.USE_RS_PROFILE</name>
                </property>
            </activation>
            <repositories>
                <repository>
                    <id>confluent-codeartifact-stabilization</id>
                    <url>${env.ORG_GRADLE_PROJECT_mavenUrl}</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                      <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>confluent-codeartifact-stabilization</id>
                    <url>${env.ORG_GRADLE_PROJECT_mavenUrl}</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
</settings>
