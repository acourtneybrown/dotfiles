NoHostAuthenticationForLocalhost yes
CanonicalizeHostname yes
CanonicalDomains notcharlie.com

Host *
 AddKeysToAgent yes
 IdentityFile ~/.ssh/id_rsa
 IgnoreUnknown UseKeychain
 UseKeychain yes

# Personal
Host *.notcharlie.com
  ForwardAgent yes
  User abrown
Host exhubber
  User adambrown
Host pfsense
  ForwardAgent no
  User admin
Host retropie*
  User pi
Host picluster-*
  User pi
Host synology
  Port 2222
Host synology-root
  Hostname synology.notcharlie.com
  IdentityFile ~/.ssh/synology
  Port 2222
  User root
Host gitea
  Hostname synology.notcharlie.com
  Port 2234
  User git
Host unifi-*
  ForwardAgent no
  User unifi-admin
